<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp5.Pages.AddClass"
             Title="Add your classes">
    
    <StackLayout>
        <Entry Placeholder="Class ID" x:Name="ClassIDEntry" />
        <Entry Placeholder="Class Name" x:Name="ClassNameEntry" />
        <DatePicker x:Name="ScheduleDatePicker" />
        <Entry Placeholder="Instructor ID" x:Name="InstructorIDEntry" />
        <Button Text="Add Class" />
    </StackLayout>
    
</ContentPage>